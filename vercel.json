{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/health/(.*)",
      "dest": "main.py"
    },
    {
      "src": "/api/costing/(.*)",
      "dest": "main.py"
    },
    {
      "src": "/docs",
      "dest": "main.py"
    },
    {
      "src": "/openapi.json",
      "dest": "main.py"
    },
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "functions": {
    "main.py": {
      "maxDuration": 300
    }
  },
  "env": {
    "PYTHONPATH": "."
  }
} 